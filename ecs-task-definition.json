{
    "containerDefinitions": [
        {
            "name": "ootd",
            "image": "826421662185.dkr.ecr.ap-northeast-2.amazonaws.com/ootd:latest",
            "cpu": 256,
            "memory": 1024,
            "portMappings": [
                {
                    "name": "ootd-8080-tcp",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "AWS_S3_REGION",
                    "value": "ap-northeast-2"
                },
                {
                    "name": "AWS_S3_BUCKET",
                    "value": "ootd-image"
                },
                {
                    "name": "SPRING_PROFILES_ACTIVE",
                    "value": "prod"
                },
                {
                    "name": "spring.jpa.hibernate.ddl-auto",
                    "value": "validate"
                },
                {
                    "name": "mongo.uri",
                    "value": "mongodb+srv://javaProject4:7jOUQCOqI5yROfx@javaproject4.6rgddex.mongodb.net/logs"
                },
                {
                    "name": "mongo.collection",
                    "value": "logs"
                },
                {
                    "name": "MONGODB_URI_LOG",
                    "value": "mongodb+srv://javaProject4:7jOUQCOqI5yROfx@javaproject4.6rgddex.mongodb.net/logs"
                },
                {
                    "name": "MONGO_COLLECTION",
                    "value": "logs"
                },
                {
                    "name": "MONGODB_URI",
                    "value": "mongodb+srv://javaProject4:7jOUQCOqI5yROfx@javaproject4.6rgddex.mongodb.net/ootd"
                },
                {
                    "name": "NOTIFICATIN_DB",
                    "value": "ootd"
                },
                {
                    "name": "GOOGLE_APP_PWD",
                    "value": "dummy_pwd"
                },
                {
                    "name": "GOOGLE_CLIENT_ID",
                    "value": "dummy_client_id"
                },
                {
                    "name": "GOOGLE_CLIENT_SECRET",
                    "value": "dummy_client_secret"
                },
                {
                    "name": "KAKAO_API_KEY",
                    "value": "dummy_kakao_key"
                },
                {
                    "name": "WEATHER_API_KEY",
                    "value": "dummy_weather_key"
                }
            ],
            "secrets": [
                {
                    "name": "SPRING_DATASOURCE_URL",
                    "valueFrom": "arn:aws:ssm:ap-northeast-2:826421662185:parameter/ootd/database/url"
                },
                {
                    "name": "SPRING_DATASOURCE_USERNAME",
                    "valueFrom": "arn:aws:ssm:ap-northeast-2:826421662185:parameter/ootd/database/username"
                },
                {
                    "name": "SPRING_DATASOURCE_PASSWORD",
                    "valueFrom": "arn:aws:ssm:ap-northeast-2:826421662185:parameter/ootd/database/password"
                },
                {
                    "name": "AWS_S3_ACCESS_KEY",
                    "valueFrom": "arn:aws:ssm:ap-northeast-2:826421662185:parameter/ootd/s3/access-key"
                },
                {
                    "name": "AWS_S3_SECRET_KEY",
                    "valueFrom": "arn:aws:ssm:ap-northeast-2:826421662185:parameter/ootd/s3/secret-key"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/ootd-task",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8080/actuator/health || exit 1"
                ],
                "interval": 60,
                "timeout": 30,
                "retries": 5,
                "startPeriod": 300
            }
        }
    ],
    "family": "ootd-task",
    "taskRoleArn": "arn:aws:iam::826421662185:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::826421662185:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}